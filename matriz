matriz = [[1,2,3],[4,5,6],[16,17,18]]
while True:
  andar = int(input("qual andar desejado?"))
  quarto = int(input("qual quarto desejado?"))

  reserva = matriz[quarto-1][andar-1]
  if reserva !=0:
     print("quarto ja esta reservado!")
  else:
     print("quarto disponivel")
     print("farei ha reserva!")
     matriz[quarto-1][andar-1]
     print("quarto reservado com sucesso!") 
